<aside class="aside col-span-2 h-screen border-double border-r-4 border-slate-50">
    <div class="container mx-auto">
        <img class="logo ml-1"
            src="https://cdn.discordapp.com/attachments/1104109106660380773/1106400779176247396/LoremLogo.png">
        <!-- Put object below -->
        <h2 class="subtitle flex justify-center pt-4">{{username}}</h2>
        <div>
            <div class="player ms-5 ps-2 me-5 border-b-4 pt-6">Player</div>
            <div>
                <button id="profile-btn" class="profile inline-flex items-center p-1 ml-1 gap-2">
                    <span class="container">
                        <img class="profileImg"
                            src="https://cdn.discordapp.com/attachments/1104109106660380773/1106461314118852669/0Ut41Y.png"
                            alt="image-description" width="20" height="20">
                    </span>Profile</button>
            </div>
            <div>
                <button id="backpack-btn" class="backpack inline-flex items-center p-1 ml-1 gap-2"><span class="container">
                        <img class="backpackImg"
                            src="https://cdn.discordapp.com/attachments/1104109106660380773/1106447590695911444/frame_3_delay-0.15s.gif"
                            alt="image-description" width="20" height="20">  
                    </span>Backpack</button>
            </div>
            <div>
                <button id="shop-btn" class="shop inline-flex items-center p-1 ml-1 gap-2"><span class="container">
                        <img class="shopImg"
                            src="https://cdn.discordapp.com/attachments/1104109106660380773/1106447590326800484/frame_3_delay-0.08s.gif"
                            alt="image-description" width="20" height="20">
                    </span>Shop</button>
            </div>
        </div>
        <div>
            <div class="skills ms-5 ps-2 me-5 border-b-4 pt-4">Skills</div>
            <div>
                <button id="woodcutting-btn" class="woodcutting inline-flex items-center p-1 ml-1 gap-2"><span
                        class="container">
                        <img class="woodcuttingImg"
                            src="https://cdn.discordapp.com/attachments/1104109106660380773/1106416009474211850/ddp8kr0-ede82974-6bb9-4469-b1cb-7c0f6f5d2902.png"
                            alt="image-description" width="20" height="20">
                    </span>Woodcutting</button>
            </div>
            <div>
                <button id="fishing-btn" class="fishing inline-flex items-center p-1 ml-1 gap-2"><span class="container">
                        <img class="fishingImg"
                            src="https://cdn.discordapp.com/attachments/1104109106660380773/1106416174826262588/b24005333b8d0bf9391096f573ce4b4e.png"
                            alt="image-description" width="20" height="20">
                    </span>Fishing</button>
            </div>

        </div>
    </div>
</aside>

<section class="container mx-auto col-start-5 grid flex self-center col-end-10 h-[60%]" id="oldNode">
    <div class="generalProfile flex flex-col justify-center">
        <h1 class="profileTitle  flex justify-center ">Welcome, Duchess</h1>
        <div class="text-center  mt-3">Time Away: 2Hrs</div>
        <div class="grid grid-cols-2 mt-3">
            <!-- Set up object -->
            <div class="text-center col-span-2">Account Details:</div>
            <div class="profileInfo text-center border-t-2 border-white ml-2 my-1 p-1">Total Skill Level</div>
            <!-- Set up object -->
            <div class="profileInfo text-center border-t-2 border-white mr-2 my-1 p-1">{{totalSkill}}</div>
            <div class="profileInfo text-center border-t-2 border-white ml-2 my-1 p-1">Total XP</div>
            <!-- Set up object -->
            <div class="profileInfo text-center border-t-2 border-white mr-2 my-1 p-1">{{totalEXP}}</div>
            <div class="profileInfo text-center border-t-2 border-white ml-2 my-1 p-1">Account Age </div>
            <!-- Set up object -->
            <div class="profileInfo text-center border-t-2 border-white mr-2 my-1 p-1">??</div>
        </div>
        <div class="grid grid-cols-2 mt-4">
            <div class=" text-center col-span-2">Since last login:</div>
            <div class="profileInfo text-center border-t-2 border-white ml-2 my-1 p-1">XP gained: 18473</div>
            <div class="profileInfo text-center border-t-2 border-white mr-2 my-1 p-1">Resources gathered: 543</div>
            <div class="profileInfo text-center border-t-2 border-white ml-2 my-1 p-1 mb-3">Woodcutting levels gained: 1
            </div>
            <div class="profileInfo text-center border-t-2 border-white mr-2 my-1 p-1 mb-3">FIshing levels gained: 2
            </div>
        </div>
    </div>
</section>


