<button class="woodcuttingCard flex justify-center self-center m-4">
    <div class="insideWoodcuttingCards">
        <div class="treeText flex shrink justify-center m-2">{{name}}</div>
        <div>
            <div class="treeContainer">
                <div class="flex justify-center content-center m-1" role="progressbar" aria-valuenow="10"
                    aria-valuemin="0" aria-valuemax="100" style="--value:10"><img class="treeImage mb-24"
                        src="https://images-wixmp-ed30a86b8c4ca887773594c2.wixmp.com/f/db21b3bc-8d2c-40ed-b627-0f9cdfe340b6/ddp8kr0-ede82974-6bb9-4469-b1cb-7c0f6f5d2902.gif?token=eyJ0eXAiOiJKV1QiLCJhbGciOiJIUzI1NiJ9..eWBS6G3TnHeg0UaujtN1xrYnlDm8evusa4v46ja3KZU"
                        alt="image-description" width="200" height="200"></div>
                 {{#if_eq id activeTree}}
                <div class="percentText flex justify-center mt-2">0% DONE</div>
                {{/if_eq}}
            </div>
        </div>

    </div>
</button>
